  "components": {
    "computeConfigs": [
      {
        "name": "yolox-deploy",
        "versions": {
          "dtlpy": "1.89.11"
        },
        "runtime": {
          "podType": "regular-s",
          "versions": {
            "dtlpy": "1.89.11"
        },
          "concurrency": 1,
          "runnerImage": "dataloopai/dtlpy-agent:gpu.cuda.11.8.py3.8.pytorch2",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 1,
            "queueLength": 100
          }
        }
      },
      {
        "name": "yolox-train-evaluate",
        "versions": {
          "dtlpy": "1.89.11"
        },
        "runtime": {
          "podType": "regular-m",
          "versions": {
            "dtlpy": "1.89.11"
        },
          "concurrency": 1,
          "runnerImage": "dataloopai/dtlpy-agent:gpu.cuda.11.8.py3.8.pytorch2",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 1,
            "queueLength": 100
          }
        }
      }
    ],
    "modules": [
      {
        "name": "yolox-module",
        "entryPoint": "model_adapter.py",
        "className": "ModelAdapter",
        "computeConfig": "yolox-deploy",
        "description": "YoloX Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "evaluate_model",
            "computeConfig": "yolox-deploy",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              },
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              }
            ],
            "output": [],
            "displayName": "Evaluate a Model",
            "displayIcon": "",
            "description": "Function to evaluate YoloX model performance"
          },
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Function to run YoloX inference on items"
          },
          {
            "name": "train_model",
            "computeConfig": "yolox-train-evaluate",
            "input": [
              {
                "type": "Model",
                "name": "model",
                "description": "Dataloop Model Entity"
              }
            ],
            "output": [],
            "displayName": "Train a Model",
